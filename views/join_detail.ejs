
        <section class="sec_w1400">
            <article class="w960">
                <div class="w480">
                    <form>
                        <label>아이디</label>
                        <input type="text" name="username"><br>
                        <label>비밀번호</label>
                        <div class="lock_wrap">
                            <input type="password" name="password">
                            <span class="lock"></span>
                        </div>
                        <label>비밀번호 재확인</label>
                        <div>
                            <input type="password" name="password_check">
                            <span class="lock_2"></span>
                        </div>
                        <label>이름</label>
                        <input type="text" name="uname"><br>
                        <div class="birth">
                            <h3><label for="yy">생년월일</label></h3>
                            <div class="bir_wrap">
                                    <span class="box">
                                        <input type="text" id="year" placeholder="년(4자)">
                                            <select name="month">
                                                <option value="">-- 선택 --</option>
                                                <option value="1">1</option>
                                                <option value="2">2</option>
                                                <option value="3">3</option>
                                                <option value="4">4</option>
                                                <option value="5">5</option>
                                                <option value="6">6</option>
                                                <option value="7">7</option>
                                                <option value="8">8</option>
                                                <option value="9">9</option>
                                                <option value="10">10</option>
                                                <option value="11">11</option>
                                                <option value="12">12</option>
                                            </select>
                                            <input type="text" id="day" placeholder="일">
                                    </span>
                            </div>
                        </div>
                        <label>성별</label>
                        <select class="gender">
                            <option value="female">여자</option>
                            <option value="man">남자</option>
                        </select>
                        <label>가입유형</label>
                        <select class="gender">
                            <option value="female">개인</option>
                            <option value="man">식품셀러</option>
                            <option value="man">프리마켓셀러</option>
                        </select>
                        <span class="choice">
                            <p>본인 확인 이메일</p>
                        </span>
                        <span class="signup-input">
                            <input class="signup-email" type="text" placeholder="선택입력"></input>
                        </span>
                    </div>
                    <div class="w480">
                        <p>휴대전화</p>
                        <span class="signup-input">
                            <select class="signup-country" class="selectbox" name="country">
                                <option value="ko">대한민국 +82</option>
                            </select>
                        </span>
                        <div class="number">
                                <input class="signup-phone" type="text" placeholder="전화번호 입력"></input>
                                <button class="cnum-btn">인증번호 받기</button>
                        </div>
                       
                        <span class="signup-input-c">
                            <input class="signup-cnum" type="text" placeholder="인증번호 입력하세요"></input>
                        </span>
                    </div>
                    <div class="w480">
                        <div class="signup-btn-wrap">
                            <button class="signup-btn">가입하기</button>
                        </div>
                    </form>
                </div>
            </article>
        </section>